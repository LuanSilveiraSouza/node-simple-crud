<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" type="text/css" href="/css/styles.css">
  <title>List</title>
</head>
<body>
  <% if (data) {%>
    <% data.forEach(row => { %>
      <h2><%= row.id %> <%= row.name %></h2>
    <% }) %>
  <% } %>
</body>
</html>